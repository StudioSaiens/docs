        {
          "anchor": "Scripts",
          "href": "https://scripts.saiens.fr",
          "icon": "slack"
        },
        {
          "anchor": "Blog",
          "href": "https://www.saiens.fr",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hello@saiens.fr"
      }
    ],
    "primary": {
      "type": "button",
